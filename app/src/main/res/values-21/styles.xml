<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base">
        <item name="android:colorPrimary">#ff3a3a3a</item>
        <!-- colorPrimaryDark is used for the status bar -->
        <item name="android:colorPrimaryDark">#8d0a0c</item>
        <!-- colorAccent is used as the default value for colorControlActivated
        which is used to tint widgets -->
        <item name="android:colorAccent">@color/red_compreingressos</item>
        <!-- You can also set colorControlNormal, colorControlActivated
        colorControlHighlight and colorSwitchThumbNormal. -->

        <item name="android:textColorPrimary">@color/red_compreingressos</item>

    </style>



</resources>
